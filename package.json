{
  "name": "qualifiedio-tests",
  "version": "1.0.0",
  "description": "test libraries",
  "main": "data/server.js",
  "scripts": {
    "start": "set DEBUG=server & nodemon data/server.js",
    "test": "mocha --reporter progress test/testSetup.js test/palindrome-test.js"
  },
  "keywords": [
    "tests"
  ],
  "engines": {
