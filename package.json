    "body-parser": "1.18.3",
    "bootstrap": "4.3.1",
    "chai": "4.2.0",
    "chalk": "2.4.2",
    "debug": "4.1.1",
    "ejs": "2.6.1",
    "express": "4.16.4",
    "jquery": "3.3.1",
    "jsdom": "14.0.0",
    "mocha": "6.1.1",
    "mongoose": "5.5.0",
    "morgan": "1.9.1",
    "nodemon": "1.18.10",
    "pug": "2.0.3",
    "request": "2.88.0",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "babel-core": "6.26.3",
    "babel-preset-latest": "6.24.1",
